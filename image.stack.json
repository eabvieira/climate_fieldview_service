{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "ECRrepositoryName": {
            "Type": "String",
            "Default": "climate_fieldview_service"
        }
    },
    "Resources": {
        "BaseServiceECR": {
            "Type": "AWS::ECR::Repository",
            "Properties": {
                "RepositoryName": {
                    "Fn::Sub": "${ECRrepositoryName}"
                }
            }
        }
    }
}